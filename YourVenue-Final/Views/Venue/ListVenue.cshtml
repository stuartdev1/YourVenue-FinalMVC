@{
    ViewData["Title"] = "List Venue";
}

<head>
    <link rel="stylesheet" href="~/css/listvenue.css" />
</head>
<main>
    <h1>List YourVenue</h1>
    <form id="ListVenueForm">
        <label for="VenueName">Venue Name</label>
        <input type="text" id="VenueName" placeholder="Venue Name" />

        <label for="VenuePhone">Venue Phone</label>
        <input type="text" id="VenuePhone" placeholder="Venue Phone" />

        <label for="VenueAddress">Venue Address</label>
        <input type="text" id="VenueAddress" placeholder="Venue Address" />

        <label for="VenueCity">Venue City</label>
        <input type="text" id="VenueCity" placeholder="Venue City" />

        <label for="VenueState">Venue State</label>
        <input type="text" id="VenueState" placeholder="Venue State" />

        <button>Submit Venue</button>
    </form>

</main>